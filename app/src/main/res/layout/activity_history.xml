<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dell.sccs_app.history">

    <android.support.v7.widget.Toolbar
        android:id="@+id/mtoolbar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/background"
        android:minHeight="?attr/actionBarSize"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:popupTheme="@style/AppTheme.PopupOverlay"
        app:theme="@style/AppTheme.AppBarOverlay">

    </android.support.v7.widget.Toolbar>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/uploadnum"
        android:textSize="20dp"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/mtoolbar" />

    <TextView
        android:id="@+id/addcontrolnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="40dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="40dp"
        android:layout_height="37dp"
        app:srcCompat="@drawable/icons_controller_gray"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/addcontrolnum"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/addcontrolnum"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/addcontrolnum"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView12" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/drawer_shadow"
        app:layout_constraintTop_toTopOf="@+id/addcontrolnum"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/addcontrolnum"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.483"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/addlampnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="100dp"
        android:layout_marginTop="40dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="33dp"
        android:layout_height="37dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintLeft_toLeftOf="@+id/addlampnum"
        app:layout_constraintRight_toRightOf="@+id/addlampnum"
        app:layout_constraintTop_toTopOf="@+id/imageView8"
        app:srcCompat="@drawable/icons_pot_1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/deletenum"
        android:textSize="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView3" />

    <TextView
        android:id="@+id/deletecontrolnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="40dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="@+id/addcontrolnum"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="40dp"
        android:layout_height="37dp"
        app:srcCompat="@drawable/icons_controller_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/deletecontrolnum"
        android:layout_marginRight="8dp"
        app:layout_constraintVertical_bias="0.51"
        app:layout_constraintRight_toRightOf="@+id/deletecontrolnum"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/deletecontrolnum" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/drawer_shadow"
        app:layout_constraintBottom_toBottomOf="@+id/deletecontrolnum"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="@+id/deletecontrolnum"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/deletecontrolnum"
        android:layout_marginLeft="8dp"
        app:layout_constraintRight_toLeftOf="@+id/deletelampnum"
        android:layout_marginRight="8dp" />

    <TextView
        android:id="@+id/deletelampnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:gravity="center"
        android:text="TextView"
        app:layout_constraintLeft_toLeftOf="@+id/addlampnum"
        app:layout_constraintTop_toTopOf="@+id/deletecontrolnum" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="33dp"
        android:layout_height="37dp"
        app:srcCompat="@drawable/icons_pot_1"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/deletelampnum"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/deletelampnum" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="353dp"
        android:layout_height="21dp"
        app:srcCompat="@drawable/shape_line"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@+id/mtoolbar"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="60dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />


</android.support.constraint.ConstraintLayout>
